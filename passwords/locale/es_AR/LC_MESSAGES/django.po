#: passwords/validators.py:30
#, python-format
msgid "Invalid Length (%s)"
msgstr "Longitud inválida (%s)"

#: passwords/validators.py:40
#, python-format
msgid "Must be %s characters or more"
msgstr "Deben ser por lo menos %s caracteres"

#: passwords/validators.py:44
#, python-format
msgid "Must be %s characters or less"
msgstr "Deben ser %s caracteres o menos"

#: passwords/validators.py:48
#, python-format
msgid "Must be more complex (%s)"
msgstr "Debe ser mas compleja (%s)"

#: passwords/validators.py:78
#, python-format
msgid "Must contain %(UPPER)s or more uppercase characters"
msgstr "Debe contener %(UPPER)s o más caracteres en mayúscula"

#: passwords/validators.py:82
#, python-format
msgid "Must contain %(LOWER)s or more lowercase characters"
msgstr "Debe contener %(LOWER)s o más caracteres en minúscula"

#: passwords/validators.py:86
#, python-format
msgid "Must contain %(DIGITS)s or more digits"
msgstr "Debe contener %(DIGITS)s o más dígitos"

#: passwords/validators.py:90
#, python-format
msgid "Must contain %(PUNCTUATION)s or more punctuation character"
msgstr "Debe contener %(PUNCTUATION)s o más signos de puntuación"

#: passwords/validators.py:94
#, python-format
msgid "Must contain %(NON ASCII)s or more non ascii characters"
msgstr "Debe contener %(NON ASCII)s o más caracteres no ascii"

#: passwords/validators.py:98
#, python-format
msgid "Must contain %(WORDS)s or more unique words"
msgstr "Debe contener %(WORDS)s o más palabras únicas"

#: passwords/validators.py:103
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Demasiado similar a [%(haystacks)s]"

#: passwords/validators.py:139
msgid "Based on a dictionary word."
msgstr "Basada en un diccionario de palabras"

#: passwords/validators.py:155
msgid "Based on a common sequence of characters"
msgstr "Basado en una secuencia común de caracteres"
